# AWS Credentials for Amazon Bedrock
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_REGION=us-east-1

# Application Ports
FRONTEND_PORT=3000
GATEWAY_PORT=3001
DATA_PROCESSOR_PORT=3002
SUMMARIZER_PORT=3003
WEB_SEARCH_PORT=3004

# Mastra Server URL (for A2A communication)
MASTRA_SERVER_URL=http://localhost:4111

# Bedrock Model Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-5-sonnet-20240620-v1:0

# Langfuse Configuration for Tracing and Observability
# Get these from your Langfuse dashboard at https://cloud.langfuse.com
LANGFUSE_PUBLIC_KEY=pk-lf-your-public-key-from-langfuse-dashboard
LANGFUSE_SECRET_KEY=sk-lf-your-secret-key-from-langfuse-dashboard

# Langfuse Base URL - choose your region
# EU Region (default)
LANGFUSE_BASEURL=https://cloud.langfuse.com
# US Region
# LANGFUSE_BASEURL=https://us.cloud.langfuse.com
# Self-hosted
# LANGFUSE_BASEURL=https://your-langfuse-instance.com

# Agent Configuration
# Gateway Agent
AGENT_ID=gateway-agent-01
AGENT_NAME=Gateway Agent

# Data Processor Agent (for data-processor container)
# AGENT_ID=data-processor-agent-01
# AGENT_NAME=Data Processor Agent

# Summarizer Agent (for summarizer container)
# AGENT_ID=summarizer-agent-01
# AGENT_NAME=Summarizer Agent

# Agent URLs for A2A Communication
DATA_PROCESSOR_URL=http://data-processor:3002
SUMMARIZER_URL=http://summarizer:3003
WEB_SEARCH_URL=http://web-search:3004

# Web Search Configuration
# Brave Search API Key (optional - fallback to DuckDuckGo if not provided)
BRAVE_SEARCH_API_KEY=your-brave-search-api-key-here

# Environment
NODE_ENV=development